<div class="flexbox">
    {{#each responseFromAPI}}
    <div id="image-box">
    <img src={{this.image_url}} alt="">
    </div>
    <div>
    <h2 class="names">{{this.name}}</h2>
    <h5 class="taglines">{{this.tagline}}</h5>
    <p>{{this.description}}</p>
    
    <h4>Food Paring:</h4>
    <ul>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <h4>Brewers Tips:</h4>
    <p>{{this.brewers_tips}}</p>
    </div>
    {{/each}}
</div>